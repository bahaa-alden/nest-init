{
  "name": "nest-init",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf build",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "prestart:prod": "yarn build",
    "start:prod": "ENV=production node build/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "pretest:e2e": "yarn db:test:restart",
    "test:e2e": "dotenv -e .env.test -- jest --watch --no-cache --config ./test/jest-e2e.json",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "redis:queue:up": "docker compose up -d redisqueue",
    "redis:queue:rm": "docker compose rm -s -f -v  redisqueue",
    "redis:queue:restart": "yarn redis:queue:rm && yarn redis:queue:up",
    "db:dev:up": "docker compose up -d dev-db",
    "db:dev:rm": "docker compose rm -s -f -v dev-db",
    "db:dev:restart": "yarn db:dev:rm && yarn db:dev:up",
    "db:rq:up": "yarn db:dev:up && yarn redis:queue:up",
    "db:rq:rm": "yarn db:dev:rm && yarn redis:queue:rm",
    "db:rq:restart": "yarn db:dev:restart && yarn redis:queue:restart",
    "db:create": "ts-node -r dotenv/config ./node_modules/typeorm-extension/bin/cli.cjs db:create  -r src/config/database/postgres --initialDatabase=postgres",
    "db:drop": "ts-node -r dotenv/config ./node_modules/typeorm-extension/bin/cli.cjs db:drop  -r src/config/database/postgres --initialDatabase=postgres",
    "db:seed:fake": "ts-node -r dotenv/config ./node_modules/typeorm-extension/bin/cli.cjs seed:run  -r src/config/database/postgres",
    "db:seed:create": "ts-node ./node_modules/typeorm-extension/bin/cli.cjs seed:create  --name  src/database/seeders/category/category.seeder.ts",
    "db:seed:real": "ts-node -r tsconfig-paths/register src/seed.ts",
    "db:seed:prod": "ENV=production ts-node -r tsconfig-paths/register src/seed.ts",
    "db:seed": "yarn db:create && yarn db:seed:real && yarn db:seed:fake",
    "doc:generate": "npx compodoc -p tsconfig.doc.json -s",
    "doc:run": "compodoc -s"
  },
  "dependencies": {
    "@casl/ability": "^6.5.0",
    "@hapi/joi": "^17.1.1",
    "@liaoliaots/nestjs-redis": "^9.0.5",
    "@nestjs-modules/mailer": "^1.10.3",
    "@nestjs/bull": "^10.0.1",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.2",
    "@nestjs/core": "^9.0.0",
    "@nestjs/devtools-integration": "^0.1.5",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/mapped-types": "^1.2.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^3.0.4",
    "@nestjs/swagger": "^7.0.3",
    "@nestjs/typeorm": "^10.0.0",
    "@nestjsx/crud-typeorm": "^5.0.0-alpha.3",
    "@types/hapi__joi": "^17.1.9",
    "@types/uuid": "^9.0.1",
    "argon2": "^0.30.3",
    "blurhash": "^2.0.5",
    "bull": "^4.12.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cloudinary": "^1.41.0",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "ioredis": "^5.3.2",
    "node-schedule": "^2.1.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.0",
    "pug": "^3.0.2",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.2.0",
    "sharp": "^0.32.6",
    "tiny-invariant": "^1.3.1",
    "txtgen": "^3.0.6",
    "typeorm": "^0.3.17",
    "typeorm-extension": "^3.2.0",
    "uuid": "^9.0.0",
    "webpack": "^5.0.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.23",
    "@faker-js/faker": "^8.3.1",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.1",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "18.16.12",
    "@types/passport-jwt": "^4.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.5.0",
    "morgan": "^1.10.0",
    "pactum": "^3.5.1",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.1.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
